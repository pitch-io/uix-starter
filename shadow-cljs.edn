{:deps true
 :builds {:app
          {:target :react-native
           :init-fn app.core/init
           :output-dir "app"
           :compiler-options {:source-map-path "app/"
                              :source-map-include-sources-content true
                              :warnings-as-errors true}
           :devtools {:preloads [preload]
                      :build-notify preload/build-notify}}}}
